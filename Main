import pygame
blue=[0,0,255]
red=[255,0,0]

x1=300
y1=300

x1_change=0
y1_change=0

pygame.init()
dis = pygame.display.set_mode((1920, 1250))

pygame.display.set_caption('Змейка')
game_over = True
while game_over:
    for event in pygame.event.get():
        if event.type == pygame.QUIT:
            game_over = False
        print(event)
    pygame.draw.rect(dis, blue, [500, 350,100,100])
    pygame.display.update()
pygame.quit()
quit()
